<ul class="list">
    {% for post in collections.posts %}
    <li>
       <div class="tag_list">
            {%- for topic in post.data.topics %}
                <small>{{ topic }}</small>
            {%- endfor %}
            </div>
                    <a href="{{ post.url }}">{{ post.data.title }}</a>

    </li>
    {% endfor %}
</ul>